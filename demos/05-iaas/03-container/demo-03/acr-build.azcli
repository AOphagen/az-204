env=32294
grp=az204-m05-containers
loc=westeurope
acr=configacr$env
img=config-ui

az acr login --name $acr
az acr update -n $acr --admin-enabled true

cd config-api
az acr build --image config-api --registry $acr --file Dockerfile .
cd ..

cd config-ui
az acr build --image config-ui --registry $acr --file Dockerfile .
cd ..