grp=az204-m05-containers
loc=westeurope
acr=az204foodacr
fqdn=azurecr.io
app=ng-config
img=$acr.$fqdn/$app
apiurl=net-config.westeurope.azurecontainer.io

az group create -n $grp -l $loc

pwd=$(az acr credential show -n $acr --query passwords[0].value -o tsv)

az container create -g $grp -l $loc -n $app --image $img --cpu 1 --memory 1 --dns-name-label $app --port 80 --registry-username $acr --registry-password $pwd --environment-variables 'ENV_API_URL'='https://'$apiurl